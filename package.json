{
  "name": "ng-q-plus",
  "version": "1.1.2",
  "description": "A module enhancing $q promises with additional features",
  "keywords": ["angularjs", "requirejs", "q", "promise", "browserify", "$q"],
  "homepage": "https://github.com/dbartholomae/ngQplus",
  "bugs": "https://github.com/dbartholomae/ngQplus/issues",
  "license": "MIT",
  "author": "Daniel Bartholomae <daniel@bartholomae.name>",
  "dependencies": {},
  "peerDependencies": {
    "angular": "1.*"
  },
  "devDependencies": {
    "npm-build-tools": "*",
    "coffee-script": "1.*"
  },
  "main": "lib/ng-q-plus.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dbartholomae/ngQplus.git"
  },
  "scripts": {
    "pretest": "npm run build",
    "test": "npm run test:requirejs && npm run test:global && npm run test:browserify",
    "test:requirejs": "node_modules/karma/bin/karma start karma.requirejs.conf.coffee",
    "test:global": "node_modules/karma/bin/karma start karma.global.conf.coffee",
    "test:browserify": "node_modules/karma/bin/karma start karma.browserify.conf.coffee",
    "build": "node_modules/coffee-script/bin/coffee -c -o lib/ src/"
  }
}
